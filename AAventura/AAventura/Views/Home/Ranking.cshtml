        <td>
            @Html.DisplayFor(modelItem => item.NrRespostasCertas)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.NrRespostasErradas)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.VoltasDadas)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.TempoTotal)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.HighScore)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.UserId }) |
            @Html.ActionLink("Details", "Details", new { id=item.UserId }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.UserId })
        </td>
    </tr>
