        </th>
        <th>
            @Html.DisplayNameFor(model => model.NrRespostasErradas)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.VoltasDadas)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.TempoTotal)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.HighScore)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
